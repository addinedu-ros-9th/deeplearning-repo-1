{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Login Response",
    "type": "object",
    "properties": {
        "user_name": {
            "type": "string",
            "description": "로그인된 사용자 이름",
            "minLength": 1
        },
        "result": {
            "type": "string",
            "enum": [
                "succeed",
                "fail"
            ],
            "description": "로그인 결과"
        }
    },
    "required": [
        "user_name",
        "result"
    ],
    "additionalProperties": false
}