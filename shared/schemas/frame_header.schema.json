{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Frame Header",
    "type": "object",
    "properties": {
        "frame_id": {
            "type": "integer",
            "minimum": 0,
            "description": "프레임 고유 번호"
        },
        "timestamp": {
            "type": "string",
            "format": "date-time",
            "description": "프레임 생성 시간 (ISO 8601 형식)"
        }
    },
    "required": [
        "frame_id",
        "timestamp"
    ],
    "additionalProperties": false
}